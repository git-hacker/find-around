<view class="page">

  <view class="weui-cells weui-cells_after-title">
    <view wx:for="{{auther}}" wx:for-item="obj"  class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image class="myheader" src="{{obj.headimg}}"></image>
      </view>
      <view class="weui-cell__bd">
        <text class="realname">{{obj.name}}</text>
        <text class="level">{{obj.level}}</text>        
      </view>
      <view class="weui-cell__ft weui-cell__ft_in-access">
        <image class="tapicon" src="/img/wx_icon.png" bindtap='copycode' data-wx='{{obj.wxcode}}'></image>
      </view>
    </view>
  </view>

</view>